    <sequenceFlow id="flow4" name="确认" sourceRef="exclusivegateway1" targetRef="endevent1"></sequenceFlow>
    <sequenceFlow id="flow5" name="驳回" sourceRef="exclusivegateway1" targetRef="adjustLostTime"></sequenceFlow>
    <userTask id="adjustLostTime" name="损时调整" activiti:assignee="${applyUserId}"></userTask>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow6" name="取消申请" sourceRef="adjustLostTime" targetRef="endevent1"></sequenceFlow>
    <sequenceFlow id="flow7" name="继续申请" sourceRef="adjustLostTime" targetRef="lostTimeApply"></sequenceFlow>
  </process>
